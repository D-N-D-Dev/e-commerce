{
  "name": "clepo",
  "version": "1.0.0",
  "description": "E-commerce",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch sass/main.scss:css/style.css",
    "build:sass": "sass  sass/main.scss:css/main.css",
    "concat:css": "concat -o css/concat.css css/main.css css/reset.css ",
    "prefixer:css": "postcss --use autoprefixer -b 'last 5 versions' css/concat.css -o css/prefix.css ",
    "compress": "css-minify --file css/concat.css css/style.css",
    "dev": "parcel index.html",
    "build": "parcel build index.html"
  },
  "author": "Thomas Ejembi",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "concat": "^1.0.3",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^8.3.0",
    "sass": "^1.26.11"
  },
  "dependencies": {}
}
